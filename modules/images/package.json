{
  "name": "@loaders.gl/images",
  "version": "2.3.0-alpha.6",
  "description": "Framework-independent loaders and writers for images (PNG, JPG, ...)",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "3d",
    "mesh",
    "point cloud",
    "PLY"
  ],
  "types": "src/index.d.ts",
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/es6/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
  "pre-build": "npm run build-bundle && npm run copy-worker && npm run build-bundle -- --env.dev",
  "build-bundle": "webpack --display=minimal --config ../../scripts/bundle.config.js",
  "copy-worker": "cp ./src/workers/get-image-data.worker.js ./dist/get-image-data.worker.js"
},
  "dependencies": {
    "@loaders.gl/loader-utils": "2.3.0-alpha.6"
  }
}
